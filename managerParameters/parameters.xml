<?xml version='1.0' encoding='utf-8'?>
<object-stream>
    <java.lang.Class>
        <!--path to the folder with json folders-->
        <jsonSource>output/jsonFolders</jsonSource>
        <!--profile that will start-->
        <profileName>3_06</profileName>
        <!--path to the NewVision jar file. як виявилося джарнік повинен
            лежати в одній папці з менеджером-->
        <newVisionPath>NewVision.jar</newVisionPath>
        <!--path to the video that OpenPose will process-->
        <videoSource>input/</videoSource>
        <!--destination of the output of the OpenPose & OpenPoseManager-->
        <videoDestination>output/</videoDestination>
        <!--OpenPose arguments-->
        <arguments>-process_real_time</arguments>
        <!--name of current OpenPose executable file-->
        <openPose>OpenPoseDemo.exe</openPose>
        <deleteProcessedJsonFolder>False</deleteProcessedJsonFolder>
        <!--If True every json folder after zipping will be deleted-->
        <deleteUploadedZippedJsons>False</deleteUploadedZippedJsons>
        <!--If True every zip archive will be deleted after upload to server-->
        <!--URL to send POST request-->
        <postURL>https://vision.ecsv.org.ua/json-store/upload</postURL>
        <!--URL to send GET request-->
        <getURL>https://vision.ecsv.org.ua/json-store/get</getURL>
    </java.lang.Class>
</object-stream>